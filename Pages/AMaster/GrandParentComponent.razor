<div>
    <h3>GrandParent Component</h3>
    <p>Property: @GrandParentProperty</p>
    <ParentComponent Property="GrandParentProperty" OnValueChanged="ParentValueChanged" />
</div>

@code {
    public string GrandParentProperty { get; set; } = "Initial Value";

    private void ParentValueChanged(string value)
    {
        GrandParentProperty = value;
    }
}
